root = true

[*]
charset                  = utf-8
end_of_line              = lf
insert_final_newline     = true
trim_trailing_whitespace = true
quote_type               = single

[*.js]
indent_style   = space
indent_size    = 2
max_line_width = 140

[*.css]
indent_style   = space
indent_size    = 2
max_line_width = 140

