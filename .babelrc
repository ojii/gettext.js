{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["ie >= 11", "last 2 versions"],
        "node": "current"
      }
    }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
